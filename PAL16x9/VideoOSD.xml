				<ondown>50</ondown>
				<onleft>-</onleft>
				<onright>-</onright>
				<content>
					<item id="1">
						<description>Skip Back</description>
						<label>31062</label>
						<label2>31062</label2>
						<onclick>playercontrol(previous)</onclick>
						<property name="previous">true</property>
					</item>
					<item id="2">
						<description>Rewind</description>
						<label>31036</label>
						<label2>31036</label2>
